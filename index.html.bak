        text-decoration: underline;
    }

    .md-heading {
        font-size: 26px;
        font-weight: 700;
        margin-bottom: 20px;
        color: #222;
        border-bottom: none; 
    }
    .md-heading i {
        margin-right: 10px;
        color: #444;
        width: 30px; 
        text-align: center;
    }

    .sub-heading {
        font-size: 18px; 
        font-weight: bold; 
        border-bottom: 2px solid #f0f0f0; 
        padding-bottom: 10px; 
        margin-bottom: 15px; 
        margin-top: 30px;
        color: #333;
    }
    .sub-heading i {
